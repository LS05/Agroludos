<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<import resource="classpath:agroludos/beans/request.xml" />
	<import resource="classpath:agroludos/beans/businessdelegate.xml" />
	<import resource="classpath:agroludos/beans/commandmapper.xml" />
	<import resource="classpath:agroludos/beans/transferobject.xml" />
	<bean id="agroludosAC" class="agroludos.presentation.controller.ApplicationController">
		<property name="cmdMap" ref="cmdMapper" />
	</bean>
	<bean id="acFactory" class="agroludos.presentation.controller.ApplicationControllerFactory" />
	<bean id="generalAC" factory-bean="acFactory" factory-method="createGeneralAC"
		parent="agroludosAC">
		<property name="agroBD" ref="agroludosBD" />
		<property name="toFact" ref="toFactory" />
	</bean>
	<bean id="userAC" factory-bean="acFactory" factory-method="createUserAC"
		parent="agroludosAC" />
</beans>