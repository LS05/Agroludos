<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<import resource="classpath:agroludos/beans/utility.xml" />
	<import resource="classpath:agroludos/beans/commands.xml" />
	<import resource="classpath:agroludos/beans/navigator.xml" />
	
	<bean id="acFactory"
		class="agroludos.presentation.controller.ACFactoryImpl"></bean>

	<bean id="agroludosAC" class="agroludos.presentation.controller.AgroludosAC"
		factory-bean="acFactory" factory-method="getAC">
		<property name="commandFactory" ref="cmdFactory" />
		<property name="commandProcessor" ref="cmdProcess" />
		<property name="nav" ref="navigator" />
	</bean>

</beans>