<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<import resource="classpath:agroludos/beans/system.xml" />
	<import resource="classpath:agroludos/beans/utility.xml" />
	
	<bean id="xmlDaoFactory" class="agroludos.integration.dao.file.xml.XmlDAOFactoryImpl">
		<constructor-arg ref="systemConf" />
		<constructor-arg ref="utilityXml" />
	</bean>

	<bean id="xmlConfigurazioneDAO" 
		  class="agroludos.integration.dao.file.xml.XmlConfigurazioneDAO">
		  <constructor-arg ref="systemConf" />
		  <constructor-arg ref="utilityXml" />
	</bean>
	
</beans>